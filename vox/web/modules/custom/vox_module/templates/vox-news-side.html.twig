<div>
  {% for node in data %}
  <div id="list">
    <img src="{{ file_url(node.field_news_image.entity.fileuri) }} " class="listevent-img">
    <div class="event-desc">
      <h5>
        {{ node.field_publish_date.value | date("d F Y") }}
      </h5>
      <h3>
        {{ node.title.value }}
      </h3>
      <a href="/node/{{ node.id }}">
        detail
      </a>
    </div>
  </div>
  {% endfor %}
</div>
